  "devDependencies": {
    "bower": "^1.3.12",
    "broccoli": "^0.16.7",
    "broccoli-concat": "^3.2.2",
    "co": "^4.6.0",
    "mocha": "^3.0.2",
    "mocha-jshint": "^2.3.1"
  },
  "dependencies": {
    "broccoli-persistent-filter": "^1.0.3",
    "hash-for-dep": "^1.0.2",
